@model IEnumerable<EComCSharp.Models.Category>

<h1>Category List</h1>
<table class="table table-bordered table-striped">
    <tr>
        <th>Category Name</th>
        <th>Display Order</th>
    </tr>
    
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.DisplayOrder</td>
        </tr>
    }
</table>
